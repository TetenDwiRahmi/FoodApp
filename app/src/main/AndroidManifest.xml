<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobile.foodapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Activity.AkunActivity"
            android:theme="@style/NoActionBar"></activity>
        <activity
            android:name=".Activity.DetailOrderActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.OrderActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.PembayaranActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.AlamatActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.CheckoutActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.KeranjangActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.ProfilActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.RegistrasiActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.MainActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.DetailActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.KategoriActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Activity.LoginActivity"
            android:theme="@style/NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>